<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
   
  <navigation-rule>
        <from-view-id>/indexLogin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>encontrado</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalido</from-outcome>
            <to-view-id>/indexLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>/indexLogin.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexCadastro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>encontrado</from-outcome>
            <to-view-id>/indexLogin.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalido</from-outcome>
            <to-view-id>/indexCadastro.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>meusImoveis</from-outcome>
            <to-view-id>/meusImoveis.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>invalido</from-outcome>
            <to-view-id>/indexCadastro.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
    
    
</faces-config>